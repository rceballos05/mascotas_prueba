@page "/usuarios"
@inject UserNavgationData userDataService

<AppBarHome LoginUser="userLoged" />
<NavigationBarFrm/>
<SistemaVeterinario.Components.Components.Usuarios.FrmUsuarios/>


@code {
    private object user = new object();
    private LoginResponse userLoged = new();

